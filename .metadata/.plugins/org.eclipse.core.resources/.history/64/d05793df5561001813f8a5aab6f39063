	public String getIsShowName() {
		try {
			if(isShow != null){
				isShowName = ConfigUtil.dictionaryMap.get(
						PropertyUtil.getProperties("whether")).get(isShow);
			}
		} catch (IOException e) {
			e.printStackTrace();
		}
		return isShowName;
	}

	public void setIsShowName(String isShowName) {
		this.isShowName = isShowName;
	}

